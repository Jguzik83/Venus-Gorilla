<h2> Edit this Survey </h2>

<div>
  <%= form_for @survey do |f| %>
    <div>
      <%= f.label :title %>
      <%= f.text_field :title %>
    </div>
    <div>
      <%= f.label :description %>
      <%= f.text_field :description%>
    </div>
    <div>
      <%= f.submit 'Save' %>
    </div>
</div>

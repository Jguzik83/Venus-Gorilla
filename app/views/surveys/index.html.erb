<h2>Please select a Survey to take!</h2>

<div>
  <% @surveys.each do |survey| %>
  <div>
    <%= survey.title %>
  </div>
  <div>
    <%= button_to 'info', survey_path(survey), method: :get %>
  </div>
  <% end %>
</div>

<h2>Or create a new Survey</h2>

<%= button_to 'Create Survey', new_survey_path, method: :get %>

